{
  "name": "gatsby-source-sanity-transform-images",
  "version": "0.0.4",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-transformer-plugin",
    "sanity",
    "gatsby-sanity-source",
    "images"
  ],
  "license": "MIT",
  "author": "React GraphQL Academy<hello@reactgraphql.academy>",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "test": "./node_modules/.bin/jest"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "babel-preset-gatsby-package": "^0.2.4",
    "babel-jest": "^24.9.0",
    "cross-env": "^5.2.1",
    "jest": "^24.9.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  }
}
